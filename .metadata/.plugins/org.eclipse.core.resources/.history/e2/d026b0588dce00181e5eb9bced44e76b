import sqlite3
#import sqlalchemy




conn = sqlite3.connect("recipe_db.db")
c = conn.cursor()

chicken_getter = c.execute("SELECT COUNT(*) FROM (SELECT `_rowid_`,* FROM `recipe_table` WHERE `ingredients` LIKE '%chicken%' ESCAPE '\' ORDER BY `_rowid_` ASC)
SELECT `_rowid_`,* FROM `recipe_table` WHERE `ingredients` LIKE '%chicken%' ESCAPE '\' ORDER BY `_rowid_` ASC LIMIT 0, 50000;")

for item in chicken_getter:
    print(chicken_getter.fetch())
    
print(chicken_getter)