import sqlite3
#import sqlalchemy


count = 0

conn = sqlite3.connect("recipe_db.db")
c = conn.cursor()

chicken_getter = c.execute("SELECT * FROM 'recipe_table' WHERE 'ingredients' like '%chicken%' ")

recipes = c.fetchall()

for item in recipes:
    print(recipes[item].fetch())
    
print(type(chicken_getter))

print(chicken_getter)


